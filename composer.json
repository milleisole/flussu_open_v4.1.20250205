{
    "name": "milleisole/flussuserver",
    "description": "A Flussu Server.",
    "keywords": [
        "flussu",
        "server"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-imagick": "^3.6",
        "ext-zip": "*",
        "dompdf/dompdf": "^2.0",
        "guzzlehttp/guzzle": "^7.8.0",
        "guzzlehttp/psr7": "^2.6.1",
        "juanantoniodelgado/stopwords": "^1.3",
        "masterminds/html5": "^2.8",
        "nikic/php-parser": "^4.13",
        "openai-php/client": "^0.7.8",
        "phpmailer/phpmailer": "^6.5",
        "phpoffice/phpspreadsheet": "^2.1",
        "shuchkin/simplexlsxgen": "^1.2",
        "stripe/stripe-php": "^14.3",
        "symfony/browser-kit": "^5.4",
        "symfony/panther": "^1.1",
        "vlucas/phpdotenv": "^5.6",
        "web64/php-nlp-client": "^0.40.5",
        "zircote/swagger-php": "^3.0"
    },
    "autoload": {
        "classmap": [
        ],
        "files": [
        ],
        "psr-4": {
            "App\\": "src/",
            "Flussu\\": "src/Flussu/",
            "Api\\": "src/Flussu/Api/"
        }
    },
    "autoload-dev": {
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
        ],
        "post-create-project-cmd": [
        ],
        "post-autoload-dump": [
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        },
        "platform-check": false,
        "platform": {
            "php": "8.1.0",
            "php-64bit": "8.1.0",
            "ext-json": "8.1",
            "ext-mbstring": "8.1"
        }
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.3"
    }
}
